{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "standard",
      "next",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "indent": ["error", 4],
      //"react/react-in-jsx-scope": "off"
      "jsx-quotes": "off",
      "quotes": "off"
  }
}